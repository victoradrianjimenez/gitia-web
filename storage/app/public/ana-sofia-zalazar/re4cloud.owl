        <termSubject rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Operating System Disk</termSubject>
    </owl:NamedIndividual>
    


    <!-- http://gitia.org/assets/ana-sofia-zalazar/re4cloud#MST-VM-SERCOM -->

