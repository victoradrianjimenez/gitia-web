            <owl:Restriction>
                <owl:onProperty rdf:resource="http://gitia.org/assets/ana-sofia-zalazar/re4cloud#isImpliedByServiceResource"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://gitia.org/assets/ana-sofia-zalazar/re4cloud#ServiceResource"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://gitia.org/assets/ana-sofia-zalazar/re4cloud#relatesScalability"/>
