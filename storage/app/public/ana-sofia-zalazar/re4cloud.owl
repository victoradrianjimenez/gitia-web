        <termSubject rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Cloud Server Fees</termSubject>
    </owl:NamedIndividual>
    


    <!-- http://gitia.org/assets/ana-sofia-zalazar/re4cloud#RSK-SRV-DCNETW -->

