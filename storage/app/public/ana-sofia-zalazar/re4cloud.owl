    <!-- http://gitia.org/assets/ana-sofia-zalazar/re4cloud#MST-VM-CHGSLA -->

    <owl:NamedIndividual rdf:about="http://gitia.org/assets/ana-sofia-zalazar/re4cloud#MST-VM-CHGSLA">
