    </owl:NamedIndividual>
    


    <!-- http://gitia.org/assets/ana-sofia-zalazar/re4cloud#MST-VM-EXTCON -->

    <owl:NamedIndividual rdf:about="http://gitia.org/assets/ana-sofia-zalazar/re4cloud#MST-VM-EXTCON">
        <rdf:type rdf:resource="http://gitia.org/assets/ana-sofia-zalazar/re4cloud#Definition"/>
        <termDescription rdf:datatype="http://www.w3.org/2001/XMLSchema#string">&quot;External Connectivity&quot; is bi-directional network traffic over supported protocols such as HTTP and HTTPS that can be sent and received from a public IP address.</termDescription>
        <termSubject rdf:datatype="http://www.w3.org/2001/XMLSchema#string">External Connectivity</termSubject>
    </owl:NamedIndividual>
    


