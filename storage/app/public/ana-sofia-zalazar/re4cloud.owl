    <!-- http://gitia.org/assets/ana-sofia-zalazar/re4cloud#RSK-SRV-SLAEXC -->

    <owl:NamedIndividual rdf:about="http://gitia.org/assets/ana-sofia-zalazar/re4cloud#RSK-SRV-SLAEXC">
        <rdf:type rdf:resource="http://gitia.org/assets/ana-sofia-zalazar/re4cloud#Limitation"/>
        <termDescription rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Customer is not entitled to a credit if he is in breach of his services agreement with Rackspace (including his payment obligations to Rackspace) until he has cured the breach. Customer is not entitled to a credit if downtime would not have occurred but for customer&apos;s breach of his agreement with Rackspace or his misuse of the Services. Customer is not entitled to a credit for downtime or outages resulting from denial of service attacks, virus activity, hacking attempts, or any other circumstances that are not within Rackspace&apos;s control.</termDescription>
        <termSubject rdf:datatype="http://www.w3.org/2001/XMLSchema#string">SLA Exclusion</termSubject>
    </owl:NamedIndividual>
    


    <!-- http://gitia.org/assets/ana-sofia-zalazar/re4cloud#RSK-SVR-CSHWAR -->

    <owl:NamedIndividual rdf:about="http://gitia.org/assets/ana-sofia-zalazar/re4cloud#RSK-SVR-CSHWAR">
        <rdf:type rdf:resource="http://gitia.org/assets/ana-sofia-zalazar/re4cloud#Warranty"/>
        <termDescription rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Rackspace guaranties the functioning of all cloud server hosts including compute, storage, and hypervisor. If a cloud server host fails, it guaranties that restoration or repair will be complete within one hour of problem identification.</termDescription>
        <termSubject rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Cloud Server Host</termSubject>
    </owl:NamedIndividual>
    


    <!-- http://gitia.org/assets/ana-sofia-zalazar/re4cloud#RSK-SVR-DCIWAR -->

    <owl:NamedIndividual rdf:about="http://gitia.org/assets/ana-sofia-zalazar/re4cloud#RSK-SVR-DCIWAR">
